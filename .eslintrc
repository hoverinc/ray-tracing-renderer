{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "ignorePatterns": ["build/**/*.js", "scenes/**/*.js", "jest.config.js", "rollup.config.js"],
  "rules": {
    "no-console": 0,
    "semi": 1
  }
}
